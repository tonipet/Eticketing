
@{
    ViewBag.Title = "AddGroup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Modified Js/GroupAccess.js"></script>

<ol class="breadcrumb">
    <li class="breadcrumb-item">
        <button type="button" class="btn btn-info" id="AddUserGroup"> Add User Group</button>
    </li>
</ol>


<div class="box">
    <div class="box-body table-responsive no-padding">
        <div class="box-body">

            <table id="TBLUserGroup" class="table table-hover" style="width:100%">
                <thead>
                    <tr>
                        <th>User Group</th>
                        <th>Edit</th>
                    </tr>
                </thead>
                <tbody></tbody>
                <tfoot>
                    <tr>
                        <th>User Group</th>
                        <th>Edit</th>
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</div>



<div class="modal fade" role="dialog" id="ModAddUserGroup">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header btn-info">
                <h5 class="modal-title">User Access List</h5>

            </div>
            <div class="box-body">
                <input type="text" id="UACode" style="width: 100%;" />
                <label>User Group:</label>
                <span class="error" id="UGccessErr">*</span>
                <input type="text" class="form-control " id="UGroup" style="width: 100%;"/>
            </div>
            <div class="modal-footer">
                <input id="UGUpdate" type="button" value="Update" class="btn btn-primary btn-flat" data-dismiss="modal">
                <input id="UGSave" type="button" value="Save" class="btn btn-primary btn-flat">
                <button type="button" id="UGClose" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>